        print("\033[38;5;248m╬"+48*"═"+1*"╬")
        faxel_w = whatsapp_api + numero
        enregistrement = open('faxel.txt','w')
        enregistrement.write('#Lien WhatsApp générer par Faxel\n#Twitter https://twitter.com/Faxel2020\n\n'+faxel_w+'\n\n')
        enregistrement.close()
        os.system('cp faxel.txt /sdcard/')
        os.system('rm -rf faxel.txt')
        print("\033[38;5;248m╬"+48*"═"+1*"╬")
        mael("\033[1;91m[\033[1;93m**\033[1;91m] \033[38;5;214mFaxel\033[1;97m un jour,    \033[38;5;214mFaxel \033[1;97mtoujours      \033[1;91m[\033[1;93m**\033[1;91m]")
        print("\033[38;5;248m╬"+48*"═"+1*"╬")
        raw_input("\n\033[1;91m[\033[1;97mRetour \033[1;91m]")
        Menu()
