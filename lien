      whatsapp_web =("https://whatsapp.com/")
      whatsapp_api =("https://api.whatsapp.com/send?phone=")
      if not numero:
        print("\n\033[1;91m[\033[1;93m*\033[1;91m] Veuillez saisir un numéro whatsapp!")
        raw_input("\n\033[1;91m[ \033[1;97mRetour \033[1;91m]")
